    "pca=PCA(n_components=2)\n",
    "\n",
    "reduced=pca.fit_transform(scaled)\n",
    "df[\"reduced2D\"]=reduced.tolist()\n",
    "\n",
    "pca=PCA(n_components=30)\n",
    "\n",
    "reduced30=pca.fit_transform(scaled)\n",
    "df[\"reduced30D\"]=reduced30.tolist()"
